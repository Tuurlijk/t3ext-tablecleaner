{
  "name": "michielroos/tablecleaner",
  "type": "typo3-cms-extension",
  "description": "Removes [deleted/hidden] records older than [N] days from tables",
  "keywords": [
    "tablecleaner",
    "analyze",
    "debug",
    "inspect",
    "health",
    "performance"
  ],
  "require": {
    "typo3/cms": "^6.2"
  },
  "require-dev": {
    "mikey179/vfsStream": "~1.6.4",
    "namelesscoder/typo3-repository-client": "~1.3.1",
    "nimut/testing-framework": "~1.1.8",
    "phpunit/phpunit": "^5.7"
  },
  "config": {
    "bin-dir": ".Build/bin",
    "vendor-dir": ".Build/vendor"
  },
  "replace": {
    "tablecleaner": "self.version",
    "typo3-ter/tablecleaner": "self.version"
  },
  "autoload": {
    "psr-4": {
      "MichielRoos\\Tablecleaner\\": "Classes"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MichielRoos\\Tablecleaner\\Tests\\": "Tests",
      "TYPO3\\CMS\\Core\\Tests\\": ".Build/vendor/typo3/cms/typo3/sysext/core/Tests",
      "TYPO3\\CMS\\Fluid\\Tests\\": ".Build/vendor/typo3/cms/typo3/sysext/fluid/Tests"
    }
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/public"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "mkdir -p .Build/public/typo3conf/ext/",
      "[ -L .Build/public/typo3conf/ext/tablecleaner ] || ln -snvf ../../../../. .Build/public/typo3conf/ext/tablecleaner"
    ]
  },
  "license": "GPL-2.0+"
}
